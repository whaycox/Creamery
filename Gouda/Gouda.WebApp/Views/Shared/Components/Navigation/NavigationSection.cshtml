@model NavigationSection
<gouda-nav-section>
    <div class="title"><gouda-label key="@Model.Label" /></div>
    <ul class="node-list">
        @foreach (var viewModel in Model.ViewModels)
        {
            <li><gouda-viewModel view-model="viewModel" /></li>
        }
    </ul>
</gouda-nav-section>